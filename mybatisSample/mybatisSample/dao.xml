<?xml version="1.0" encoding="utf-8"?>

<sqlMap namespace="" 
        xmlns="http://ibatis.apache.org/mapping" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >

  <alias>
    <typeAlias alias="Categories" type="WebApp1.Categories" />
  </alias>

  <statements>
    <select id="select_categories" resultClass="Categories" >
      select  CategoryID,
              CategoryName,
              Description
        from  Categories
    </select>
  </statements>

</sqlMap>
